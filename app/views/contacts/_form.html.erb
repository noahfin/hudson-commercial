
<%= simple_form_for(@contact) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :prefix %>
    <%= f.input :first_name %>
    <%= f.input :middle_intial %>
    <%= f.input :last_name %>
    <%= f.input :title %>
    <%= f.input :cell %>
    <%= f.input :phone %>
    <%= f.input :fax %>
    <%= f.input :bussiness_email %>
    <%= f.input :personal_email %>
    <%= f.input :extra_email %>
    <%= f.input :user_id %>
    <%= f.input :category_id %>
    <%= f.input :company_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
