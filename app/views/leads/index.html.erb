<p id="notice"><%= notice %></p>

<h1>Leads</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Company</th>
      <th>Location requirement</th>
      <th>Space type</th>
      <th>Size requirment</th>
      <th>Budget requirment</th>
      <th>Assigned to</th>
      <th>Leadable</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.first_name %></td>
        <td><%= lead.last_name %></td>
        <td><%= lead.phone %></td>
        <td><%= lead.email %></td>
        <td><%= lead.company %></td>
        <td><%= lead.location_requirement %></td>
        <td><%= lead.space_type %></td>
        <td><%= lead.size_requirment %></td>
        <td><%= lead.budget_requirment %></td>
        <td><%= lead.assigned_to %></td>
        <td><%= lead.leadable %></td>
        <td><%= lead.user %></td>
        <td><%= link_to 'Show', lead %></td>
        <td><%= link_to 'Edit', edit_lead_path(lead) %></td>
        <td><%= link_to 'Destroy', lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lead', new_lead_path %>
