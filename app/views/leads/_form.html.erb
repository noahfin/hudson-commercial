
<%= simple_form_for(@lead) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :phone %>
    <%= f.input :email %>
    <%= f.input :company %>
    <%= f.input :location_requirement %>
    <%= f.input :space_type %>
    <%= f.input :size_requirment %>
    <%= f.input :budget_requirment %>
    <%= f.input :assigned_to %>
    <%= f.association :leadable %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
