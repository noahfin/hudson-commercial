
<%= simple_form_for(@touch) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :way_of_contact %>
    <%= f.input :success %>
    <%= f.input :date_touched %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
