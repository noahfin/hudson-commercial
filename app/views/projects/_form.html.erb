
<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :todo %>
    <%= f.association :user %>
    <%= f.association :contact %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
